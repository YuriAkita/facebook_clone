<h1>FacebookCloneにログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.submit "ログイン" %>
  <p>または</p>
  <%= link_to "新しいアカウントを作成", new_user_path %>
<% end %>
